{
  "name": "{project-name}",
  "version": "{project-version}",
  "description": "{project-description}",
  "homepage": "{project-url}",
  "repository": {
    "type": "git",
    "url": "{project-url}"
  },
  "main": "dist/index.js",
  "scripts": {
    "dist": "npm run dist.css && npm run dist.js && npm run dist.img",
    "dist.js": "webpack",
    "dist.css": "node-sass src/sass/style.scss dist/css/style.css  --include-path node_modules --output-style compressed",
    "dist.img": "imagemin src/img/* --out-dir=dist/img",
    "dist.watch": "concurrently --kill-others \"npm run dist.watch.css\" \"npm run dist.watch.js\"",
    "dist.watch.js": "nodemon -w src -e js -x \"npm run dist.js\"",
    "dist.watch.css": "nodemon -w src -e scss -x \"npm run dist.css\"",
    "dist.watch.img": "nodemon -w src/img -x \"npm run dist.img\"",
    "start": "npm run dist.watch && npm run server",
		"server": "http-server .",
    "test": "coffeekraken-testing-stack"
  },
  "keywords": [
    "coffee",
    "kraken",
    "coffeekraken"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "license": "MIT",
  "dependencies": {
    "coffeekraken-sugar": "git+https://git@github.com/Coffeekraken/sugar.git#release/0.0.1",
    "coffeekraken-gridle": "git+https://git@github.com/Coffeekraken/gridle.git#release/0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "coffeekraken-testing-stack": "git+https://github.com/Coffeekraken/testing-stack.git#release/0.0.1",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "imagemin-cli": "^3.0.0",
    "node-sass": "4.3.0",
    "nodemon": "^1.11.0",
    "open": "0.0.5",
    "webcomponents.js": "^0.7.23",
    "webpack": "^1.14.0"
  }
}
